usando el sistema;
Utilizando System.Collections.Generic;
Utilizando System.ComponentModel;
Utilizando System.Data;
Utilizando Sistema.Dibujo;
Utilizando System.Linq;
Utilizando System.Text;
Utilizando System.Threading.Tasks;
Utilizando System.Windows.Forms;

espacio de nombres progra16
{
    Clase parcial pública Form1: Forma
    {
        Form1 público ()
        {
            InitializeComponent ();
        }

        Privado vacío btncalcular_Click (objeto remitente, EventArgs e)
        {
            doble r = 0.0;
            doble a = 0.0;
            doble b = 0.0;

            a = Convert.ToDouble (txtA.Text);
            b = Convert.ToDouble (txtB.Text);

            if (rbSuma.Checked == true)
                r = a + b;

            if (rbResta.Checked == true)
                r = a - b;

            if (rbmultiplicacion.Checked == true)
                r = a * b;
            
            if (rbdivision.Checked == true)
                r = a / b;

            lblResultado.Text = r.ToString ();
        }
    }
}

