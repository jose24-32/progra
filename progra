espacio de nombres Deber
{
    Clase parcial pública Form5: Forma
    {
        // Instancia de la clase Leer
        leer l = nueva leer ();
        // Alamcena la ruta del archivo .txt
        cadena pública ARCHIVO = "";

        Formulario público5 ()
        {
            InitializeComponent ();
        }

        vo5 privado Form5_Load (remitente del objeto, EventArgs e)
        {

        }

        // Abre el archivo abierto Diálogo y captura de la ruta del bloque de notas
        public void cargarArchivo ()
        {
            tratar
            {
                this.openFileDialog1.ShowDialog ();

                if (! string.IsNullOrEmpty (this.openFileDialog1.FileName))
                {
                    ARCHIVO = this.openFileDialog1.FileName;
                    l.lecturaArchivo (dataGridView1, ',', ARCHIVO);

                }
            }
            atrapar (excepción ex)
            {
                MessageBox.Show ("Error:" + ex.ToString ());
            }
        }

        privado void button1_Click (objeto remitente, EventArgs e)
        {
            cargarArchivo ();
        }

        privado void dataGridView1_CellContentClick (objeto remitente, DataGridViewCellEventArgs e)
        {

        }
    }
}
 
 
 la clase
 
 usando el sistema;
Utilizando System.Collections.Generic;
Utilizando System.Linq;
Utilizando System.Text;
Utilizando System.Threading.Tasks;
Utilizando System.IO;
Utilizando System.Windows.Forms;

espacio de nombres Deber
{
    lectura de clase
    {

        // Leer el archivo y la llama al metodo agregar FilaDatagridview para que por cada línea del bloque agregue una linea en el datagridview '
        Public void lecturaArchivo (tabla DataGridView, carácter caracter, cadena ruta)
        {
            StreamReader objReader = new StreamReader (ruta);
            string sLine = "";
            int fila = 0;
            tabla.Rows.Clear ();
            tabla.AllowUserToAddRows = false;

            hacer
            {
                sLine = objReader.ReadLine ();
                if ((sLine! = null))
                {
                    si (fila == 0)
                    {
                        tabla.ColumnCount = sLine.Split (caracter) .Length;
                        nombrarTitulo (tabla, sLine.Split (caracter));
                        fila + = 1;
                    }
                    más
                    {
                        agregarFilaDatagridview (tabla, sLine, caracter);
                        fila + = 1;
                    }

                }
            }

            while (! (sLine == null));
            objReader.Close ();
        }

        privado void agregarFilaDatagridview (tabla DataGridView, string sLine, caracter caracter)
        {
            lanzar nuevo NotImplementedException ();
        }

        // Agregar el HeaderText al datagridview (SON LOS TITULOS) '
        público static void nombrarTitulo (tabla DataGridView, cadena [] títulos)
        {
            int x = 0;
            para (x = 0; x <= tabla.ColumnCount - 1; x ++)
            {
                tabla.Columns [x] .HeaderText = titulos [x];
            }
        }

        // Agrega una fila por cada línea de Bloc de notas: D '
        public static void agregarFilaDatagridview (tabla DataGridView, línea de cadena, carácter caracter, int fila)
        {
            string [] arreglo = linea.Split (caracter);
            tabla.rows.Add (arreglo);
        }

    }
}

ventanas de 15

usando el sistema;
Utilizando System.Collections.Generic;
Utilizando System.ComponentModel;
Utilizando System.Data;
Utilizando Sistema.Dibujo;
Utilizando System.Linq;
Utilizando System.Text;
Utilizando System.Threading.Tasks;
Utilizando System.Windows.Forms;

espacio de nombres Deber
{
    Clase parcial pública Form4: Forma
    {
        Formulario público 4 ()
        {
            InitializeComponent ();
        }

        persona persona1 = nueva persona ();
        Lista <persona> lista = nueva Lista <persona> ();

        Vacío público guardarpersona ()
        {
            persona persona1 = nueva persona (txt_nombre.Text, txt_apellido.Text, int.Parse (txt_edad.Text));
            lista.Add (persona1);

            dataGridView2.DataSource = lista;
            dataGridView2.DataSource = null;
        }

        public void modificar (string nombre)
        {
            foreach (var persona en lista)
            {
                si (persona.Nombre == nombre)
                {
                    persona.Nombre = txt_nombre.Text;
                    persona.Apellido = txt_apellido.Text;
                    persona.Edad = int.Parse (txt_edad.Text);
                } 
                
            }

        }

        vo4 privado vacío Form4_Load (remitente del objeto, EventArgs e)
        {

        }

        void privado btnsalir_Click (objeto remitente, EventArgs e)
        {
            Cerrar ();
        }

        privado void btnguardar_Click (objeto remitente, EventArgs e)
        {
            guardarpersona ();
        }

        privado void btn_Modificar_Click (objeto remitente, EventArgs e)
        {
            modificar (txt_nomamodificar.Text);
        }
    }
}

la clase

usando el sistema;
Utilizando System.Collections.Generic;
Utilizando System.Linq;
Utilizando System.Text;
Utilizando System.Threading.Tasks;

espacio de nombres Deber
{
    persona de clase
    {
        cadena privada nombre;
        cuerda privada apellido;
        int int privado;
        persona pública ()
        {
            nombre = string.Empty;
            apellido = string.Empty;
            edad = 0;
        }

        persona pública (string nom, string ap, int ed)
        {
            nombre = nom;
            apellido = ap;
            edad = ed;
        }


        Nombre público
        {
            obtener {volver nombre; }
            establecer {nombre = valor; }
        }

        apellido
        {
            obtener {return apellido; }
            establecer {apellido = valor; }
        }

        público int edad
        {
            obtener {return edad; }
            conjunto {edad = valor; }
        }

    }
}
